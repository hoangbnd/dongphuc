@if(ViewBag.ProductCate != null)
{
	var categoryChild = ContentHelper.TextFolder("Product~ProductCategory").CreateQuery().WhereEquals("ParentUUID", ViewBag.ProductCategory.UUID);
    if(categoryChild != null){
    	@Html.FrontHtml().RenderView("Product.ListCategoryParent",ViewData)
	}else{
		@Html.FrontHtml().RenderView("Product.ListCateChild",ViewData)
	}
	
}else
{
	@Html.FrontHtml().RenderView("Product.ListCateChild",ViewData)
}