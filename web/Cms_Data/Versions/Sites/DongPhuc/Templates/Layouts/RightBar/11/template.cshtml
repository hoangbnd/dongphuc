<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Transitional//EN" "http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd">
<html xmlns="http://www.w3.org/1999/xhtml">
<head>
  	<meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=9; IE=8; IE=7; IE=EDGE">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <title>Title</title>
  
    <script>
      cartName = "@Site.Current.Name.ToLower()";
    </script>
  
    <!-- Latest compiled and minified CSS -->
	@Html.FrontHtml().HtmlTitle()
	@Html.FrontHtml().Meta()
	@Html.FrontHtml().RegisterStyles()
    @Html.FrontHtml().RegisterScripts()
</head>
<body>
    <!-- container -->
    <div class="container">
        <div class="wrapper">
          	@Html.FrontHtml().RenderView("Header.Header",ViewData)
          	@Html.FrontHtml().RenderView("Share.FlexMenu",ViewData)
          	@Html.FrontHtml().RenderView("Share.BreadCrumb",ViewData)
            <div class="bg-color-white">
              	<div class="white-space space-mini"></div>
                <div class="col-md-9 col-fix">
                  @Html.FrontHtml().Position("Main")
                </div>
                <div class="col-md-3 col-fix">
                  @Html.FrontHtml().Position("RightBar")
                </div>
              	<div class="clearfix"></div>
            </div>
			<div class="">
              @Html.FrontHtml().Position("Bottom")
          	</div>
          	@Html.FrontHtml().RenderView("Footer.Footer",ViewData)
    	</div>
    </div>
   	@Html.FrontHtml().RenderView("Share.ShoppingCartModal",ViewData)
    <!-- /container -->
  	@Html.FrontHtml().RegisterScripts()
</body>
</html>