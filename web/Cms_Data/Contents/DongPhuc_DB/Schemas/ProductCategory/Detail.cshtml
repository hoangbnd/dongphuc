@if (Model != null)
{ 
<div>
	<h3 class="title" @ViewHelper.Edit(Model,"Title")>@Html.Raw(Model.Title ?? "")</h3>
	<div class="content">
		<div @ViewHelper.Edit(Model,"Url")>@Html.Raw(Model.Url ?? "")</div>
		<div @ViewHelper.Edit(Model,"SortBy")>@Html.Raw(Model.SortBy ?? "")</div>
		<div @ViewHelper.Edit(Model,"ListHome")>@Html.Raw(Model.ListHome ?? "")</div>
	</div>
</div>
}